{
  "name": "fablo-test-network-countrya",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "CountryA",
    "tlsEnable": true,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "CountryA": {
      "mspid": "CountryAMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/countrya.example.com/users/Admin@countrya.example.com/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.countrya.example.com"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/countrya.example.com/users/Admin@countrya.example.com/msp/signcerts/Admin@countrya.example.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.countrya.example.com": {
      "url": "grpcs://peer0.countrya.example.com:7041",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/countrya.example.com/msp/tlscacerts/tlsca.countrya.example.com-cert.pem"
      }
    }
  },
  "channels": {
    "my-channel": {
      "peers": {}
    }
  }
}
